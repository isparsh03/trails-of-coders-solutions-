fn main() {
    let n1 = 1;
    let n2 = 10;
    let mut sum_even = 0;
    let mut sum_odd = 0;

    for num in n1..=n2 {
        if num % 2 == 0 {
            println!("Even number: {}", num);
            sum_even += num;
        } else {
            println!("Odd number: {}", num);
            sum_odd += num;
        }
    }

    println!("Sum of even numbers: {}", sum_even);
    println!("Sum of odd numbers: {}", sum_odd);
}
